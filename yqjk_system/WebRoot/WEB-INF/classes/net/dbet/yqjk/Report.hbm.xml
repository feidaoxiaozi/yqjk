<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="net.dbet.yqjk">

	<class name="net.dbet.yqjk.Report" table="yqjk_report">

		
		<id name="reportId" column="reportId">
			<generator class="assigned" />
		</id>
        <property name="type" column="type"/>
		<property name="title" column="title"/>		
		<property name="resource" column="resource"/>
		<property name="published_time" column="published_time"/>
		<property name="reprinted_volume" column="reprinted_volume"/>		
		<property name="click_number" column="click_number"/>
		<property name="back_number" column="back_number"/>		
		<property name="url" column="url"/>
		<property name="author" column="author"/>		
		<property name="seach_time" column="seach_time"/>
		<property name="engineName" column="engineName"/>		
		<property name="roleId" column="roleId"/>
	</class>

</hibernate-mapping>